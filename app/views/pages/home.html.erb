<h1>Home</h1>
<p>Welcom to homepage</p>

<div class="">
  <% if current_user %>
    <p><%= link_to 'Sign out', destroy_user_session_path, method: :delete %></p>
    <p><%= link_to 'Edit account', edit_user_registration_path, method: :delete %></p>
    <br>
    <%= image_tag(current_user.avatar_thumbnail) if current_user.avatar.attached? %>
  <% else %>
    <p><%= link_to 'Create Account', new_user_registration_path %></p>
    <p><%= link_to 'Sign In', new_user_session_path %></p>
  <% end %>
</div>
